<%- include('./partials/head') %>
    
    <div class='container'>
        <%- include('./partials/header') %>
        <aside>
            <%- include('./partials/sidebar') %>
        </aside>
        <section>
            <h2><%= title %></h2>
            <hr>
            
           <form method='post'>
            <div class='form-input'>
                <label for='category'>Category: </label>
                <input type='text' name='category' id='cat-name'>
            </div>

            <div class='form-input'>
                <label for='cat-description'>Description: </label>
                <textarea name='cat-description' id='cat-description' cols='30' rows='5'></textarea>
            </div>
           </form>
        
        </section>  
<%- include('./partials/footer') %>  
